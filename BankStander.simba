procedure makeItems();
begin
  //writeln(Inventory.GetSelected());

  if not (Inventory.IsOpen) then
  begin
    writeln("["+timeElapsed+"]:" + " Opening Inventory");
    Inventory.Open();
  end;

  if Make.IsOpen() then
    Make.Select(makeAllInterface,QUANTITY_ALL);
  Sleep(3000);
  if not XPBar.EarnedXP() then
  begin
    if (Inventory.Combine(invOne, invTwo)) then
    begin
      writeln("["+timeElapsed+"]: "+"Combining [" + itemOne + "] with [" + itemTwo + "]");
      Inc(actions);
      Sleep(delay);
      if Make.IsOpen() then
        Make.Select(makeAllInterface,QUANTITY_ALL);
    end;
  end;
  Sleep(100,200);
end;

procedure bankForItems();
begin
  writeln("["+timeElapsed+"]: " + "Banking for items");
  Sleep(1000);
  bankObj.WalkInteract(['Bank']);
  Sleep(2000);

  if(Bank.IsOpen()) then
  begin
    Inventory.Items.Click(itemDeposit);
    //Sleep(1000);
    //Bank.OpenSearch();
    //Bank.Search(itemTwo);
    Sleep(1500);
    Bank.Withdraw(itemTwo);
    //Bank.CloseSearch();
    printProgReport();
    Sleep(2000);
  end;
  Bank.Close();
end;

procedure checkInventory();
begin
  itemOneCount := Inventory.Items.Count([invOne]);
  itemTwoCount := Inventory.Items.Count([invTwo]);

  if (itemTwoCount < itemTwoMin) then
  begin
    writeln("["+timeElapsed+"]: "+" Ran out of ["+itemTwo+"]");

    bankForItems();
  end;
end;



